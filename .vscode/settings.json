{
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        ".browserslistrc": false,
        ".editorconfig": false,
        ".eslintignore": false,
        ".eslintrc.json": false,
        ".github": false,
        ".nx": false,
        ".prettierignore": false,
        ".prettierrc": false,
        ".verdaccio": false,
        ".vscode": false,
        "*.gitignore": false,
        "**/.gitkeep": false,
        "dist": false,
        "node_modules": false,
        "nx.json": false,
        "package-lock.json": false
    },
    "editor.defaultFormatter": "biomejs.biome",
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "editor.codeActionsOnSave": {
        "quickfix.biome": "explicit",
        "source.organizeImports.biome": "explicit"
    },
    "workbench.editor.autoLockGroups": {
        "imagePreview.previewEditor": true
    }
}
